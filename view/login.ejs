<%- include('./partials/header.ejs') %>
    <link rel="stylesheet" type="text/css" href="/style/login.css">

    <%- include('./partials/navbar.ejs') %>
        <div class="container">
            <div class="primary">
                <div class="main first">
                    <div class="layer">
                        <h1 class=" text head  ">Good to see you agagin!</h1>
                        <h2 class="text font">Hope to de well</h2>
                        <h3 class="text font">Login with social media</h3>
                        <div class="social">
                            <img src="/icons/icons8-facebook-48.png" alt="facebook">
                            <img src="/icons/icons8-instagram-48.png" alt="twitter">
                            <img src="/icons/icons8-linkedin-48.png" alt="linkedin">
                        </div>

                    </div>


                </div>
                <div class="main">
                    <div class="logo">
                        <img src="/icons/profile.png" alt="">
                    </div>
                    <h3 class="head"><strong>WELCOME</strong></h3>
                    <div class="form">
                        <form action="#" method="post">
                            <div class="form_elm">
                                <label for="email">Email Id</label>
                                <input type="email" id="email" name="email" required placeholder="abc@xyz.com">
                            </div>
                            <div class="form_elm">
                                <label for="password">password</label>
                                <input type="password" id="password" name="password" required>
                            </div>
                            <div class="form_elm">
                                <p class="alert">Forgot password?</p>
                                <button type="submit">login</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <script src="/javascript/all.js"></script>
        <%- include('./partials/footer.ejs') %>